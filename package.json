{
    "name": "step-api-sdk-generator",
    "packageManager": "yarn@3.3.0",
    "private": true,
    "type": "module",
    "scripts": {
        "fetch": "node ./scripts/fetchServiceSpecs.mjs",
        "skeleton": "node ./scripts/generateOverrideSkeleton.mjs",
        "combine": "node ./scripts/combineSpecs.mjs",
        "build": "openapi-generator-cli generate -i gateway/$API_SPEC_FILE.json -g typescript-axios -o ../step-api-sdk --remove-operation-id-prefix --additional-properties=ngVersion=6.1.7,npmName=step-api-sdk,supportsES6=true,npmVersion=1.0.0,withInterfaces=true",
        "docs": "webpack-dev-server --open"
    },
    "dependencies": {
        "@openapitools/openapi-generator-cli": "^2.6.0",
        "axios": "^1.4.0",
        "css-loader": "^6.8.1",
        "json-loader": "^0.5.7",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "style-loader": "^3.3.3",
        "swagger-ui": "^4.19.0",
        "swagger-ui-express": "^4.6.3"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "html-webpack-plugin": "^5.5.3",
        "jest": "^29.5.0",
        "webpack": "^5.86.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "jest": {
        "verbose": true
    }
}
