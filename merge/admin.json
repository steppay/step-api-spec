                    },
                    "userRegistration": {
                        "type": "boolean"
                    },
                    "emailAsUsername": {
                        "type": "boolean"
                    },
                    "editUsername": {
                        "type": "boolean"
                    },
                    "forgotPassword": {
                        "type": "boolean"
                    },
                    "rememberMe": {
                        "type": "boolean"
                    },
                    "verifyEmail": {
                        "type": "boolean"
                    },
                    "loginWithEmail": {
